<!DOCTYPE html>
<html>
    <head>
        <title>MoveRails</title>
        <%= csrf_meta_tags %>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <%= stylesheet_link_tag    'user-feed', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'user-feed', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
        <%= render "layouts/navbar-left" %>
        <%= render "layouts/toolbar" %>
        <%= yield %>
        <%= render "layouts/footer" %>
    </body>
</html>
